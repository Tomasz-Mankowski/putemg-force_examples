<?xml version="1.0"?>

<features_calculation>
    <windowing window="512" step="256" />

    <emg_desc>
        <feature name="AAC" />            <!--Average Amplitude Change-->
        <feature name="TM" order="4" />   <!--Absolute Temporal Moment-->
        <feature name="VAR"  />           <!--Variance-->
        <feature name="LOG" />            <!--LOG Detector-->
        <feature name="RMS" />            <!--Root Mean Square-->
        <feature name="IAV" />            <!--Integral Absolute Value-->
    </emg_desc>

    <force_desc>
        <force_feature name="MEAN" />
        <force_feature name="MEDIAN" />
        <force_feature name="LAST" />
    </force_desc>
</features_calculation>

